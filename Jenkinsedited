pipeline
{
agent any
stages
{
 stage ('code scm checkout')
 { steps {  git branch: 'master', url: 'https://github.com/Amfaa/gradle-calculator.git'   } }

 stage ('assemble && code build')

 {   steps {  withGradle {
                            sh './gradlew clean'
                              './gradlew assemble'
                               './gradlew build'
                                 './gradlew jar'
                          }
             }
    }
                           
    }
    }
